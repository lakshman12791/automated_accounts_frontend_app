{
  "name": "automated-accounts-frontend-app",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "server": "node server/index.js",
    "server:dev": "nodemon --watch server --ext js,mjs,cjs,json --exec node server/index.js",
